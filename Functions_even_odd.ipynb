        "def lesser_of_two_evens(a,b):\n",
        "    if a%2==0 and b%2==0:\n",
        "      if a<b:\n",
        "        print(a)\n",
        "      else:\n",
        "        print(b)\n",
        "    else:\n",
        "      if a>b:\n",
        "        print(a)\n",
        "      else:\n",
        "        print(b)\n",
        "# Check\n",
        "lesser_of_two_evens(2,4)\n",
        "# Check\n",
        "lesser_of_two_evens(2,5)"
      ]
